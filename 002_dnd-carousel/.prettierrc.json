{
  "singleQuote": true,
  "trailingComma": "all",
  "semi": false,
  "useTabs": false,
  "endOfLine": "auto",
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "printWidth": 140,
  "arrowParens": "always",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 140
      }
    }
  ]
}
